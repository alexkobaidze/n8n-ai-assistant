      "გუშინ",
      "ბოლო",
      "დრო",
      "პერიოდი"
    ],
    "requiresAuth": true,
    "required_slots": [
      "vehicleId_or_plate",
      "from",
      "to"
    ],
    "api": {
      "method": "GET",
      "path": "/api/v1/vehicles/history",
      "auth": "bearer"
    },
    "request_mapping": {
      "query": {
        "vehicle": "{{vehicleId_or_plate}}",
        "from": "{{from}}",
        "to": "{{to}}"
      }
    },
    "response_template": "ისტორია მომზადდა მითითებული პერიოდისთვის: {{from}} - {{to}}. თუ გსურთ, შემიძლია PDF რეპორტიც დაგიგენერიროთ."
  },
  {
