    "requiresAuth": true,
    "required_slots": [
      "reportType",
      "from",
      "to",
      "vehicleId_or_plate"
    ],
    "api": {
      "method": "POST",
      "path": "/api/v1/reports/pdf",
      "auth": "bearer"
    },
    "request_mapping": {
      "body": {
        "type": "{{reportType}}",
        "vehicle": "{{vehicleId_or_plate}}",
        "from": "{{from}}",
        "to": "{{to}}"
      }
    },
    "response_template": "PDF რეპორტი მზადაა. ჩამოსატვირთი ბმული: {{url}}"
  }
